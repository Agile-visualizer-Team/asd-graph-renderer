{
    "template": "graph",
    "nodes": {
        "atom":{
            "name": "node",
            "variables": ["label","color"]
        },
        "style":{
            "color":{
                "root":"yellow",
                "leaves":"purple",
                "nonRoot":"blue"
            }
        }
    },
    "edge": {
        "atom":{
            "name": "edge",
            "variables": ["to","from","weight"]
        },
        "style":{
            "color":{
                "branch":"green",
                "path":"yellow"
            }
        }
    }
}