{
    "template": "graph",
    "nodes": {
        "atom": {
            "name": "node",
            "variables": ["label","color"]
        },
        "style": {
            "color": {
                "root": "yellow",
                "leaves": "purple",
                "nonRoot": "blue"
            }
        }
    },
    "edges": {
        "atom": {
            "name": "edge",
            "variables": ["from","to","weight"]
        },
        "style": {
            "color": {
                "branch": "green",
                "path": "yellow"
            }
        }
    }
}